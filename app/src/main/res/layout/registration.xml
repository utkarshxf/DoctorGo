<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fregments.Registration">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView2"
                android:layout_width="275dp"
                android:layout_height="33dp"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="4dp"
                android:fontFamily="@font/poppins_bold"
                android:gravity="center"
                android:text="Basic Details"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/textView3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="36dp"
                android:hint="feel free to fill your details"

                android:textSize="16sp"
                app:layout_constraintBottom_toTopOf="@+id/textView4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.497"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView2" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="317dp"
                android:layout_height="24dp"
                android:layout_marginTop="20dp"
                android:text="Gender"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.507"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/EmailcardView" />

            <Button
                android:id="@+id/menbtn"
                android:layout_width="100dp"
                android:layout_height="34dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="5dp"
                android:layout_weight="1"
                android:background="@drawable/genderselected"
                android:fontFamily="@font/poppins"
                android:text="Male"
                android:textColor="#ed8d00"
                android:textSize="12sp"
                app:layout_constraintEnd_toStartOf="@+id/femalebtn"
                app:layout_constraintStart_toStartOf="@+id/textView5"
                app:layout_constraintTop_toBottomOf="@+id/textView5"
                tools:ignore="TouchTargetSizeCheck" />


            <Button
                android:id="@+id/femalebtn"
                android:layout_width="100dp"
                android:layout_height="34dp"
                android:layout_margin="2dp"
                android:layout_marginTop="4dp"
                android:layout_weight="1"
                android:background="@drawable/genderselected"
                android:fontFamily="@font/poppins"
                android:text="Female"
                android:textColor="#ed8d00"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/otherbtn"
                app:layout_constraintHorizontal_bias="0.384"
                app:layout_constraintStart_toEndOf="@+id/menbtn"
                app:layout_constraintTop_toBottomOf="@+id/textView5"
                tools:ignore="TouchTargetSizeCheck" />

            <Button
                android:id="@+id/otherbtn"
                android:layout_width="100dp"
                android:layout_height="34dp"
                android:layout_margin="2dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="12dp"
                android:layout_weight="1"
                android:background="@drawable/genderselected"
                android:fontFamily="@font/poppins"
                android:text="other"
                android:textColor="#ed8d00"
                android:textSize="12sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="@+id/textView5"
                app:layout_constraintStart_toEndOf="@+id/femalebtn"
                app:layout_constraintTop_toBottomOf="@+id/textView5"
                tools:ignore="TouchTargetSizeCheck" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="322dp"
                android:layout_height="24dp"
                android:layout_marginTop="20dp"
                android:text="Full Name"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView3" />

            <androidx.cardview.widget.CardView
                android:id="@+id/cardView"
                android:layout_width="350dp"
                android:layout_height="53dp"
                android:layout_marginTop="4dp"
                android:background="@color/white"
                app:cardCornerRadius="10dp"
                app:layout_constraintEnd_toEndOf="@+id/textView4"
                app:layout_constraintStart_toStartOf="@+id/textView4"
                app:layout_constraintTop_toBottomOf="@+id/textView4">

                <EditText
                    android:id="@+id/EditName"
                    android:layout_width="319dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="@color/white"
                    android:hint="Name"
                    android:inputType="text"
                    android:textColor="@color/black" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView10"
                android:layout_width="316dp"
                android:layout_height="27dp"
                android:layout_marginTop="20dp"
                android:text="Email ID"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintBottom_toTopOf="@+id/EmailcardView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.455"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cardView"
                app:layout_constraintVertical_bias="0.724" />

            <androidx.cardview.widget.CardView
                android:id="@+id/EmailcardView"
                android:layout_width="350dp"
                android:layout_height="53dp"
                android:layout_marginTop="4dp"
                android:background="@color/white"
                app:cardCornerRadius="10dp"
                app:layout_constraintEnd_toEndOf="@+id/textView10"
                app:layout_constraintStart_toStartOf="@+id/textView10"
                app:layout_constraintTop_toBottomOf="@+id/textView10">

                <EditText
                    android:id="@+id/EditEmail"
                    android:layout_width="315dp"
                    android:layout_height="53dp"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="@color/white"
                    android:hint="Email ID"
                    android:inputType="text"
                    android:textColor="@color/black" />

            </androidx.cardview.widget.CardView>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="315dp"
                android:layout_height="31dp"
                android:layout_marginTop="20dp"
                android:text="Paracticing From"
                android:textSize="16sp"
                android:textStyle="bold"

                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.55"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/menbtn"
                app:layout_constraintVertical_bias="0.0" />

            <androidx.cardview.widget.CardView
                android:id="@+id/month"
                android:layout_width="127dp"
                android:layout_height="44dp"
                android:layout_marginStart="4dp"
                android:layout_marginTop="3dp"
                android:layout_marginEnd="28dp"
                android:background="@color/white"
                app:cardCornerRadius="10dp"
                app:layout_constraintEnd_toStartOf="@+id/years"
                app:layout_constraintStart_toStartOf="@+id/textView6"
                app:layout_constraintTop_toBottomOf="@+id/textView6">

                <EditText
                    android:id="@+id/textmonth"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:digits="1234567890"
                    android:maxLength="2"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="@color/white"
                    android:hint="Month"
                    android:inputType="text"
                    android:textAlignment="viewStart"
                    android:textColor="@color/black"
                    tools:ignore="TouchTargetSizeCheck" />

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/years"
                android:layout_width="127dp"
                android:layout_height="44dp"
                android:layout_marginTop="3dp"
                android:background="@color/white"
                app:cardCornerRadius="10dp"
                app:layout_constraintEnd_toEndOf="@+id/textView6"
                app:layout_constraintHorizontal_bias="0.491"
                app:layout_constraintStart_toEndOf="@+id/month"
                app:layout_constraintTop_toBottomOf="@+id/textView6">

                <EditText
                    android:id="@+id/textyear"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="8dp"
                    android:backgroundTint="@color/white"
                    android:hint="Years"
                    android:digits="1234567890"
                    android:maxLength="4"
                    android:inputType="text"
                    android:textAlignment="viewStart"
                    android:textColor="@color/black"
                    tools:ignore="TouchTargetSizeCheck" />

            </androidx.cardview.widget.CardView>

            <ImageButton
                android:id="@+id/submit"
                android:layout_width="75dp"
                android:layout_height="71dp"
                android:background="@drawable/button"
                android:elevation="9dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.501"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.875"
                app:srcCompat="@drawable/rightarrow"
                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </FrameLayout>
</layout>